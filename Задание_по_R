#Напишите функцию get_fractions, которая принимает на вход два числа, m и n,
#содержащий все дроби вида {i/m, i = 0, 1, ..., m} и {j/n, j =0, 1, ..., n}
#Вектор не должен содерать повторов; порядок убывания
get_fractions <- function(m,n) {
  # создадим переменную i, присвоем для неё вектор
  i <- seq(0, 1, 1/m) # задаём вектор в промежутке [0;1] с шагом 1/m
  # создадим переменную j, присвоем для неё вектор
  j <- seq(0,1, 1/n)  # задаём вектор в промежутке [0;1] с шагом 1/n
  c(i, j)  # команда для смещивания векторов
  w <- sort(c(i,j), decreasing = TRUE) # сортировка векторов в порядке убывания
  unique(w) # удаление повторовка
  w <- unique(w) # перезапись переменной, где храниться окончательный результат
  w
}

